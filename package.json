{
  "name": "Boilerplate",
  "version": "0.0.0",
  "description": "New project boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browserify": "~3.36.0",
    "browserify-shim": "~3.4.1",
    "connect": "~2.14.3",
    "gulp": "~3.6.0",
    "gulp-autoprefixer": "0.0.7",
    "gulp-changed": "~0.3.0",
    "gulp-csso": "^0.2.8",
    "gulp-imagemin": "~0.1.5",
    "gulp-livereload": "~1.2.0",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp-sass": "^0.7.1",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "~0.1.1"
  },
  "dependencies": {
    "jquery": "~2.1.0"
  }
}
