{
  "name": "Boilerplate",
  "version": "0.0.2",
  "description": "New project boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/bradcerasani/boilerplate.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^0.9.1",
    "browserify": "~4.1.8",
    "browserify-shim": "~3.5.0",
    "connect": "~2.19.4",
    "gulp": "~3.7.0",
    "gulp-autoprefixer": "0.0.7",
    "gulp-changed": "~0.4.0",
    "gulp-imagemin": "~0.6.0",
    "gulp-notify": "~1.3.1",
    "gulp-open": "~0.2.8",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^0.7.2",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^0.3.0",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "~0.1.1"
  },
  "dependencies": {
    "browserify": "^4.1.8",
    "browserify-shim": "^3.5.0",
    "connect": "^2.19.4",
    "gulp": "^3.7.0",
    "gulp-changed": "^0.4.0",
    "gulp-imagemin": "^0.6.0",
    "gulp-notify": "^1.3.1",
    "gulp-sass": "^0.7.2",
    "gulp-streamify": "0.0.5",
    "jquery": "~2.1.1"
  }
}
