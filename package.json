{
  "name": "Boilerplate",
  "version": "0.0.2",
  "description": "New project boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/bradcerasani/boilerplate.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^1.5.7",
    "browserify": "~6.x",
    "browserify-shim": "~3.8.0",
    "connect": "~3.2.0",
    "gulp": "~3.8.8",
    "gulp-autoprefixer": "1.0.1",
    "gulp-changed": "~1.0.0",
    "gulp-file-include": "^0.5.1",
    "gulp-imagemin": "~1.0.1",
    "gulp-notify": "~2.0.0",
    "gulp-open": "~0.3.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.0.0",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "pretty-hrtime": "^0.2.2",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "^2.0.0"
  },
  "dependencies": {
    "browserify": "^4.1.8",
    "browserify-shim": "^3.5.0",
    "connect": "^2.19.4",
    "gulp": "^3.7.0",
    "gulp-changed": "^0.4.0",
    "gulp-file-include": "^0.5.1",
    "gulp-imagemin": "^0.6.0",
    "gulp-notify": "^1.3.1",
    "gulp-sass": "^1.0.0",
    "gulp-streamify": "0.0.5",
    "jquery": "~2.1.1"
  }
}
